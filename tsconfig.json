{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}